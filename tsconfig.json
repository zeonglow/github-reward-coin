{
  "compilerOptions": {
    "allowJs": true,
    "lib": ["es2023", "dom", "dom.iterable", "esnext"],
    "target": "es2023",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "typeRoots": ["./types", "./node_modules/@types"]
  },
  "include": ["supabase/**/*", "./types"]
}
